int main()
{
    int A[5];
    int i = 0, j, tmp;
    while ((i < 5) {
        A[i] = 5 - i;
        i = i + 1;
    }

    i = 5 - 1;
    while (i >= 0) {
        j = 0;
        while (j < i - 1) {
            if (A[j] > A[j + 1]) {
                tmp = A[j];
                A[j] = A[j + 1];
                A[j + 1] = tmp;
            }
            j++;
        }
        i--;
    }

    i = 0;
    while (i < 5) {
        printInt(A[i]);
        i = i + 1;
    }
    return ;
}
