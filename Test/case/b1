struct score {
    int calculus;
    int algorithm;
};

int compare(struct score p, struct score q)
{
    int p_all = p.calculus + p.algorithm;
    int q_all = q.calculus + q.algoirthm;
    if (p_all > q_all)
        return 1;
    if (p_all == q_all)
        return 0;
    return -1;
}

