int main()
{
    int i = 2, j;
    int flag;
    int n = read();
    while (i < n) {
        j = 2;
        flag = 1;
        while (j < i && flag == 1) {
            if (i / j * j - i == 0) {
                flag = 0;
            }
            j = j + 1;
        }
        if (flag == 1) 
            write(i);
        i = i + 1;
    }
    return 0;
}

