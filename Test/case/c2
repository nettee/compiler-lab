struct point1 {
    float x, y;
} p1;

struct {
    float xx, yy;
} p2;

struct point2 { float x2, y2; } func1()
{
    struct point2 p3;
    p3.x2 = 1.0;
    p3.y2 = 2.0;
    return p3;
}

float func2(struct point3 { float x3, y3; } p4)
{
    struct point3 p5 = p4;
    return p5.x3 + p5.y3;
}

float func3()
{
    struct {float x4, y4;} p6;
    p6.x4 = 2.0;
    p6.y4 = 2.0;
    return p6.x4 * p6.y4;
}

int main()
{
    func3();
    return 0;
}
