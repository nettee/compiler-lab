int main()
{
    int leap;
    int year = read();
    if (year <= 0) {
        write(-1);
        return 0;
    } else {
        int remain4 = year - (year / 4) * 4;
        int remain100 = year - (year / 100) * 100;
        int remain400 = year - (year / 400) * 400;
        if (remain4 != 0) leap = 0;
        else if (remain400 == 0) leap = 1;
        else if (remain100 == 0) leap = 0;
        else leap = 1;
    }
    if (leap == 0) write(365);
    else write(366);
    return 0;
}
